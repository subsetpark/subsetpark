<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
   <title>Subset Park</title>
   <description>{loglevel, debug}</description>
   <link>http://blog.zdsmith.com</link>
   <atom:link href="http://blog.zdsmith.com/feed.xml"
     rel="self" type="application/rss+xml" />

    {% for post in context|sortdesc:'date' %}
        {% if post.status == "post" %}
           <item>
           <title>{{ post.title }}</title>
           <link>https://{{ host }}{{ site_root }}/posts/{{ post.title|slugify }}.html</link>
           <guid>https://{{ host }}{{ site_root }}/posts/{{ post.title|slugify }}.html</guid>
           <pubDate>{{ post.rss_date }}</pubDate>
           </item>
        {% endif %}
   {% endfor %}
</channel>
</rss>
